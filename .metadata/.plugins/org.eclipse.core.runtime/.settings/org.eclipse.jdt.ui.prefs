uACE0 \uACB0\uACFC\uB97C ResultSet \uC73C\uB85C \uBC1B\uC544\uC628\uB2E4.&\#13;\r\n         rs\=pstmt.executeQuery();&\#13;\r\n         //\uBC18\uBCF5\uBB38 \uB3CC\uBA74\uC11C ResultSet \uC758 cursor \uB97C \uD55C\uCE78\uC529 \uB0B4\uB9B0\uB2E4.&\#13;\r\n         while(rs.next()) {&\#13;\r\n            //cursor \uAC00 \uC704\uCE58\uD55C \uACF3\uC758 \uCE7C\uB7FC \uB370\uC774\uD130 \uCD94\uCD9C\uD574\uC11C \uC5B4\uB518\uAC00\uC5D0 \uB2F4\uAE30 &\#13;\r\n            &\#13;\r\n         }&\#13;\r\n         &\#13;\r\n      }catch(Exception e) {&\#13;\r\n         //\uD639\uC2DC \uC608\uC678\uAC00 \uBC1C\uC0DD\uD55C\uB2E4\uBA74 \uC608\uC678\uC815\uBCF4\uB97C \uCF58\uC194\uC5D0 \uCD9C\uB825\uD574\uC11C \uC5D0\uB7EC\uC758 \uC6D0\uC778 \uCC3E\uAE30 &\#13;\r\n         e.printStackTrace();&\#13;\r\n      }finally {&\#13;\r\n         try {&\#13;\r\n            if(rs\!\=null)rs.close();&\#13;\r\n            if(pstmt\!\=null)pstmt.close();&\#13;\r\n            if(conn\!\=null)conn.close();&\#13;\r\n         }catch(Exception e) {}&\#13;\r\n      }</template><template autoinsert\="true" context\="java" deleted\="false" description\="MemberDao\uC758 member update \uD328\uD134\uC785\uB2C8\uB2E4." enabled\="true" name\="memberupdate">  //\uC804\uCCB4 \uD68C\uC6D0\uC758 \uBAA9\uB85D\uC744 \uB9AC\uD134\uD558\uB294 \uBA54\uC18C\uB4DC &\#13;\r\n   public List&lt;MemberDto&gt; getList(){&\#13;\r\n      &\#13;\r\n      //\uD68C\uC6D0 \uBAA9\uB85D\uC744 \uB2F4\uC744 \uAC1D\uCCB4 \uC0DD\uC131&\#13;\r\n      List&lt;MemberDto&gt; list\=new ArrayList&lt;&gt;();&\#13;\r\n      &\#13;\r\n      Connection conn \= null;&\#13;\r\n      PreparedStatement pstmt \= null;&\#13;\r\n      ResultSet rs \= null;&\#13;\r\n      try {&\#13;\r\n         //Connection \uAC1D\uCCB4\uC758 \uCC38\uC870\uAC12 \uC5BB\uC5B4\uC624\uAE30 (Connection Pool \uC5D0\uC11C \uD558\uB098 \uAC00\uC838\uC624\uAE30)&\#13;\r\n         conn \= new DbcpBean().getConn();&\#13;\r\n         //\uC2E4\uD589\uD560 sql \uBB38 \uC791\uC131&\#13;\r\n         String sql \= "SELECT num, name, address"&\#13;\r\n               + " FROM member"&\#13;\r\n               + " ORDER BY num ASC";&\#13;\r\n         //sql \uBB38\uC744 \uC804\uB2EC\uD558\uBA74\uC11C PreparedStatement \uAC1D\uCCB4\uC758 \uCC38\uC870\uAC12 \uC5BB\uC5B4\uC624\uAE30&\#13;\r\n         pstmt \= conn.prepareStatement(sql);&\#13;\r\n         //? \uC5D0 \uAC12\uC744 \uBC14\uC778\uB529 \uD560\uAC8C \uC788\uC73C\uBA74 \uD55C\uB2E4.&\#13;\r\n&\#13;\r\n         //select \uBB38 \uC218\uD589\uD558\uACE0 \uACB0\uACFC\uB97C ResultSet \uC73C\uB85C \uBC1B\uC544\uC628\uB2E4.&\#13;\r\n         rs \= pstmt.executeQuery();&\#13;\r\n         //\uBC18\uBCF5\uBB38 \uB3CC\uBA74\uC11C ResultSet \uC758 cursor \uB97C \uD55C\uCE78\uC529 \uB0B4\uB9B0\uB2E4.&\#13;\r\n         while (rs.next()) {&\#13;\r\n            //cursor \uAC00 \uC704\uCE58\uD55C \uACF3\uC758 \uCE7C\uB7FC \uB370\uC774\uD130 \uCD94\uCD9C\uD574\uC11C \uC5B4\uB518\uAC00\uC5D0 \uB2F4\uAE30 &\#13;\r\n            &\#13;\r\n            //MemberDto \uAC1D\uCCB4\uB97C \uC0DD\uC131\uD574\uC11C &\#13;\r\n            MemberDto dto\=new MemberDto();&\#13;\r\n            //ResultSet \uC5D0\uC11C \uC77D\uC5B4\uC628 \uC815\uBCF4\uB97C MemberDto \uAC1D\uCCB4\uC758 setter \uBA54\uC18C\uB4DC\uB97C \uC774\uC6A9\uD574\uC11C \uB2F4\uB294\uB2E4.&\#13;\r\n            dto.setNum(rs.getInt("num"));&\#13;\r\n            dto.setName(rs.getString("name"));&\#13;\r\n            dto.setAddr(rs.getString("address"));&\#13;\r\n            //\uD68C\uC6D0 \uD55C\uBA85\uC758 \uC815\uBCF4\uB97C \uB2F4\uACE0 \uC788\uB294 MemberDto \uAC1D\uCCB4\uC758 \uCC38\uC870\uAC12\uC744 ArrayList \uAC1D\uCCB4\uC5D0 \uB204\uC801\uC2DC\uD0A4\uAE30&\#13;\r\n            list.add(dto);&\#13;\r\n         }&\#13;\r\n&\#13;\r\n      } catch (Exception e) {&\#13;\r\n         //\uD639\uC2DC \uC608\uC678\uAC00 \uBC1C\uC0DD\uD55C\uB2E4\uBA74 \uC608\uC678\uC815\uBCF4\uB97C \uCF58\uC194\uC5D0 \uCD9C\uB825\uD574\uC11C \uC5D0\uB7EC\uC758 \uC6D0\uC778 \uCC3E\uAE30 &\#13;\r\n         e.printStackTrace();&\#13;\r\n      } finally {&\#13;\r\n         try {&\#13;\r\n            if (rs \!\= null)&\#13;\r\n               rs.close();&\#13;\r\n            if (pstmt \!\= null)&\#13;\r\n               pstmt.close();&\#13;\r\n            if (conn \!\= null)&\#13;\r\n               conn.close(); //Connection \uBC18\uB0A9&\#13;\r\n         } catch (Exception e) {&\#13;\r\n         }&\#13;\r\n      }&\#13;\r\n      &\#13;\r\n      //ArrayList \uAC1D\uCCB4\uC758 \uCC38\uC870\uAC12 \uB9AC\uD134\uD558\uAE30&\#13;\r\n      return list;&\#13;\r\n   }</template></templates>
